{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"module": "ESNext",
		"moduleDetection": "force",
		"jsx": "preserve",
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"app-env.d.ts",
		"nativewind-env.d.ts"
	]
}
