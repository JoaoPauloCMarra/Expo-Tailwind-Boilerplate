		"expo-dev-client": "~3.3.8",
		"expo-device": "~5.9.3",
		"expo-font": "~11.10.2",
		"expo-haptics": "~12.8.1",
		"expo-image-picker": "~14.7.1",
		"expo-linking": "~6.2.2",
		"expo-localization": "~14.8.3",
		"expo-router": "~3.4.6",
