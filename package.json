{
  "name": "expo-tailwind-boilerplate",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "expo start",
    "start:c": "watchman watch-del-all & watchman shutdown-server && expo start -c",
    "open:ios": "open -a \"Xcode\" ios",
    "open:android": "open -a \"Android Studio\" android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "web:build": "npx expo export -p web",
    "web:deploy": "netlify deploy --dir dist",
    "web:deploy:prod": "netlify deploy --prod --dir dist",
    "native:build:all": "eas build --platform all",
    "native:build:ios": "eas build --platform ios",
    "native:build:android": "eas build --platform android",
    "pod": "bunx pod-install@latest",
    "test:unit": "jest --maxWorkers=50%",
    "test:unit:watch": "jest --watch --maxWorkers=25%",
    "test:unit:ci": "jest --runInBand",
    "expo:upgrade": "expo install --fix",
    "format": "prettier . --write",
    "lint": "eslint . --fix",
    "prepare": "husky install",
    "postinstall": "bun expo:upgrade && bunx pod-install"
  },
  "engines": {
    "node": ">=18",
    "bun": ">=1"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo/html-elements": "^0.9.1",
    "@expo/react-native-action-sheet": "^4.0.1",
    "@expo/vector-icons": "^14.0.0",
    "@hookform/resolvers": "^3.3.4",
    "@react-native-community/netinfo": "11.1.0",
    "@react-native-community/slider": "4.4.2",
    "@react-navigation/native": "^6.1.9",
    "@tanstack/react-query": "^5.18.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "expo": "~50.0.5",
    "expo-build-properties": "~0.11.1",
    "expo-camera": "~14.0.4",
    "expo-dev-client": "~3.3.8",
    "expo-device": "~5.9.3",
    "expo-font": "~11.10.2",
    "expo-haptics": "~12.8.1",
    "expo-image-picker": "~14.7.1",
    "expo-linear-gradient": "~12.7.2",
    "expo-linking": "~6.2.2",
    "expo-localization": "~14.8.3",
    "expo-router": "~3.4.6",
    "expo-secure-store": "~12.8.1",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "expo-web-browser": "~12.8.2",
    "jotai": "^2.6.4",
    "nativewind": "~4.0.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.50.1",
    "react-native": "0.73.4",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-web": "~0.19.10",
    "tailwind-merge": "^2.2.1",
    "tailwind-variants": "^0.2.0",
    "tailwindcss": "^3.4.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-transform-export-namespace-from": "^7.23.4",
    "@expo/webpack-config": "~19.0.1",
    "@tanstack/eslint-plugin-query": "^5.18.1",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.51",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@typescript-eslint/typescript-estree": "^7.0.1",
    "ajv": "^8.12.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-universe": "^12.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hook-form": "^0.3.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.14.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^9.0.10",
    "jest": "^29.3.1",
    "jest-expo": "~50.0.1",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "typescript": "^5.3.3"
  }
}
