{
	"name": "expo-boilerplate",
	"main": "expo-router/entry",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"start": "expo start",
		"start:c": "watchman watch-del-all & watchman shutdown-server && expo start -c",
		"android": "expo run:android",
		"ios": "expo run:ios",
		"web": "expo start --web",
		"prebuild": "expo prebuild",
		"web:build": "npx expo export -p web",
		"web:deploy": "netlify deploy --dir dist",
		"web:deploy:prod": "netlify deploy --prod --dir dist",
		"native:build:all": "eas build --platform all",
		"native:build:ios": "eas build --platform ios",
		"native:build:android": "eas build --platform android",
		"pod": "bunx pod-install",
		"test:unit": "jest --maxWorkers=50%",
		"test:unit:watch": "jest --watch --maxWorkers=25%",
		"test:unit:ci": "jest --runInBand",
		"expo:upgrade": "expo install --fix",
		"format": "prettier . --write",
		"lint": "eslint . --fix",
		"prepare": "husky install"
	},
	"engines": {
		"npm": "please-use-bun",
		"yarn": "please-use-bun",
		"pnpm": "please-use-bun",
		"node": ">=18",
		"bun": ">=1"
	},
	"dependencies": {
